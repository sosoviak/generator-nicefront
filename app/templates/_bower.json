  	<% if (ie8) { %>"jquery": "1.10.2",<% } else { %>"jquery": "~2.0.3",<% } %>
  	"jquery.easing": "~1.3.0",
  	<% if (transit) { %>"jquery.transit": "~0.9.9", <% } %>
